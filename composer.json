{
  "name": "tourze/user-id-bundle",
  "description": "用户身份模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "hughcube/static-instance": "^1.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/security-core": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/arrayable": "0.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\UserIDBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\UserIDBundle\\Tests\\": "tests"
    }
  }
}